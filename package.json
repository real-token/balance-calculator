    "json2csv": "^6.0.0-alpha.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^22.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
